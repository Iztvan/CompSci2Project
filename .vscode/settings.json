{
    "editor.fontFamily": "monospace, ",
    "files.autoSave": "afterDelay",
    "editor.cursorBlinking": "phase",
    "terminal.integrated.cursorStyleInactive": "line",
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.editor.enablePreview": true,
    "workbench.iconTheme": "deepdark-material-icons",
    "explorer.confirmDragAndDrop": false,
    "editor.wordWrap": "on",
    "workbench.colorCustomizations": {
        "terminal.background": "#00000000",
        "minimap.background": "#00000000",
        "scrollbar.shadow": "#00000000"
    },
    "workbench.settings.applyToAllProfiles": [
        "workbench.colorCustomizations"
    ],
    "vscode_vibrancy.opacity": -1,
    "vscode_vibrancy.refreshInterval": 10,
    "liveshare.allowGuestDebugControl": true,
    "liveshare.allowGuestTaskControl": true,
    "vscode_vibrancy.theme": "Default Dark",
    "vscode_vibrancy.enableAutoTheme": true,
    "animations.Install-Method": "Apc Customize UI++",
    "apc.imports": [
        "file://${userHome}/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.3/dist/updateHandler.js"
    ],
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Frontier Light (rainglow)",
    "vscode_vibrancy.preferedDarkTheme": "Default Dark",
    "vscode_vibrancy.preferedLightTheme": "Default Dark",
    "vscode_vibrancy.type": "auto",
    "apc.electron": {
    "opacity": 0.9,
    "transparent": false,
    "backgroundColor": "#00000000",
    "vibrancy": "ultra-dark"
    },
    "animations.Enabled": true,
    "animations.CursorAnimation": true,
    "animations.Command-Palette": "None",
    "animations.CursorAnimationOptions": {
    "TrailLength": 10
    },
    "animations.Tabs": "Slide",
    "animations.Active": "Indent",
}